{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-restricted-syntax": "off",
        "no-param-reassign": "off",
        "no-labels": "off",
        "no-constant-condition": "off",
        "no-continue": "off",
        "object-curly-newline":"off",
        "import-extensions": "off"
    }
}
